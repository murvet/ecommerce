@model IEnumerable<WebSiteExample.Models.Products>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>

    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

@{
    if (Model != null)
    {
        foreach (var item in Model)
        {
            <li class="col-lg-4 mb-4">


                <img src="~/Content/vendor/jquery/2.JPG" />
                <h2>@item.Title</h2>
                <p>
                   @item.Description
                </p>
            </li>
        }
    }
}
